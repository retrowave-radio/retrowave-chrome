{
  "manifest_version": 2,
  "name": "Retrowave Radio",
  "version": "0.1.0",
  "description": "Click for next/start; Double click for play/pause",
  "author": "Andrey Kravtsov <raidendev@gmail.com>",
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": ["*://retrowave.ru/*"]
  },
  "browser_action": {
    "38": "assets/icon-play_128.png"
  },
  "icons": {
    "128": "assets/icon-play_128.png"
  }
}
